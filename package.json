{
  "name": "zds-mappings",
  "version": "1.4.1",
  "description": "Mapping files for ZendrumStudio applications",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "build:watch": "babel src -w -d dist",
    "prepublish": "npm run build",
    "lint": "eslint '**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:cov:open": "open ./coverage/lcov-report/index.html",
    "test:cov:watch": "jest --coverage --watchAll"
  },
  "repository": "https://github.com/dkadrios/zds-mappings.git",
  "author": "Darin Kadrioski",
  "license": "MIT",
  "engines": {
    "node": "^8.10.0"
  },
  "jest": {
    "globals": {
      "__DEV__": true,
      "__TEST__": true,
      "__API__": "http://localhost:3002"
    },
    "setupFiles": [
      "./__tests__/__config__/tempPolyfills.js",
      "./__tests__/__config__/global.js",
      "./__tests__/__config__/fetch-mock.js",
      "jest-localstorage-mock"
    ],
    "modulePathIgnorePatterns": [
      "./__config__",
      "./__assets__",
      "./__mocks__"
    ],
    "moduleNameMapper": {
      "\\.(.scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "eslint": "5.0.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "gulp": "3.9.1",
    "gulp-ext-replace": "0.3.0",
    "gulp-help": "1.6.1",
    "gulp-modify": "0.1.1",
    "jest": "23.2.0",
    "rimraf": "2.6.2"
  },
  "dependencies": {}
}
